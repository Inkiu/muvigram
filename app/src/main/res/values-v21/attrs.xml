<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="FeedTabView">
        <attr name="textSize" format="integer"/>
        <attr name="textColor" format="color"/>
        <attr name="backLineColor" format="color"/>
        <attr name="activeTabColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RecyclerViewPager">
        <attr name="rvp_triggerOffset" format="float"/>
        <attr name="rvp_flingFactor" format="float"/>
        <attr name="rvp_singlePageFling" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TransCircleView">
        <attr name="dynamic_width" format="integer"/>
        <attr name="dynamic_height" format="integer"/>
        <attr name="dynamic_icon" format="integer"/>
        <attr name="dynamic_text" format="string"/>
        <attr name="dynamic_textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="DynamicImageView">
        <attr name="dynamic_width"/>
        <attr name="dynamic_height"/>
    </declare-styleable>

    <declare-styleable name="StreamTextView">
        <attr name="dynamic_textColor" format="color"/>
        <attr name="dynamic_width"/>
        <attr name="dynamic_height"/>
        <attr name="dynamic_text"/>
        <attr name="dynamic_textSize"/>
    </declare-styleable>

    <declare-styleable name="TransParentTabView">
        <attr name="textSize"/>
        <attr name="textColor"/>
        <attr name="backLineColor"/>
        <attr name="activeTabColor"/>
    </declare-styleable>

</resources>